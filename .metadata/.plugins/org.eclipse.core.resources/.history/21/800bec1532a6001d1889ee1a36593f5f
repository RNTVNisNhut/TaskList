package workspace;

import java.util.Scanner;

public class main {
	public static void main(String[] args) {
		function x = new function();
		List L = x.loadData();
		x.showData(L);
		int choose = 0;
		Scanner sc = new Scanner(System.in);
		do {
			int select = 0;
			System.out.println("1 -> Add task"
					+ "-1 -> end");
			select = Integer.parseInt(sc.nextLine());
			
			if(choose == -1) choose = 1;
		}
		while(choose == 0);

	}
}
