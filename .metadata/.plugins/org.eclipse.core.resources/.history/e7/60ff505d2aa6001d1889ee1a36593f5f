package workspace;

public class test {
	public static void main(String[] args) {
		String test = "Done App-Hard-6/2/2023";
		int indexName = test.indexOf('-');
		String nameTask = test.substring(0,indexName-1);
		int indexName2 = test.lastIndexOf('-');
		String level = test.substring(indexName+1,indexName2-1);
		String day = test.substring(indexName2+1);
		System.out.println(test + nameTask + level);
	}
	
}
